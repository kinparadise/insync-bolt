{
  "expo": {
    "name": "InSync",
    "slug": "insync-video-calling",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "insync",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to schedule meetings."
      }
    },
    "android": {
      "permissions": ["READ_CALENDAR", "WRITE_CALENDAR"]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-camera", "expo-calendar"],
    "experiments": {
      "typedRoutes": true
    }
  }
}